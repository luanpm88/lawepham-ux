{% if style == 'gallery' %}
    <hr class="mt-lvl4">
    <ul class="fnl-gallery">
        {% for row in rows %}
            {% include 'helper::_datalist_row_gallery.twig' with row %}
        {% endfor %}
    </ul>
    <hr>
{% else %}
    <hr class="mt-lvl2">
    <ul>
        {% for row in rows %}
            {% include 'helper::_datalist_row.twig' with row %}
        {% endfor %}
    </ul>
{% endif %}
